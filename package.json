{
  "name": "frontend",
  "private": true,
  "version": "0.8.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "npm run lint --fix",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-one": "vitest --run",
    "test": "vitest",
    "format": "prettier . --check",
    "format-fix": "prettier . --write",
    "check": "npm run format-fix && npm run lint-fix && npm run build && npm run test-one",
    "commit": "npm run build && git add -A && cz"
  },
  "dependencies": {
    "@pbe/react-yandex-maps": "^1.2.5",
    "@reduxjs/toolkit": "^2.0.1",
    "classnames": "^2.5.1",
    "compose-function": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-redux": "^9.0.2",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.2",
    "@storybook/addon-interactions": "^7.6.2",
    "@storybook/addon-links": "^7.6.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.6.2",
    "@storybook/react": "^7.6.2",
    "@storybook/react-vite": "^7.6.2",
    "@storybook/test": "^7.6.2",
    "@types/compose-function": "^0.0.33",
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.17",
    "@types/react-redux": "^7.1.32",
    "@types/yandex-maps": "^2.1.34",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "prettier": "3.1.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.2",
    "stylelint": "^16.0.2",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.1",
    "vite-plugin-env": "^1.0.1",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint-fix",
    "*.{js,jsx,css,md,html,json}": "npm run format-fix",
    "*": "git add -A"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
